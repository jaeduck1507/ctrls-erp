<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.project.erp.fm.mapper.BonusMapper">
	
	<resultMap id="BonusMap" type="Bonus">
		<id property="bonusNo" column="bonus_no"/>
		<result property="bonusName" column="bonus_name"/>
		<result property="bonusDesc" column="bonus_desc"/>
	</resultMap>
	
	<select id="showBonusAll" resultMap="BonusMap">
		SELECT *
		FROM bonus
	</select>
	
	
</mapper>